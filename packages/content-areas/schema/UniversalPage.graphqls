type UniSeo {
  """
  Meta title
  """
  title: String

  """
  A description of the product for usage in search engine results.
  """
  description: String
}

type UniversalPage {
  title: String
  meta: Seo
}

input UniversalPageInput {
  url: String!
}

type UniversalApi {
  pageContent(input: UniversalPageInput!): UniversalPage
}

extend type Query {
  common: UniversalApi
}
